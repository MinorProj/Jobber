<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="comorgsminorproj.httpsgithub.jobber.Resume">

    <Button
        android:id="@+id/btFile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Choose  file"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="138dp"
        android:layout_marginLeft="74dp"
        android:layout_marginStart="74dp"
        android:layout_marginTop="93dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btUpload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upload"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="138dp"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:layout_alignBaseline="@+id/btFile"
        android:layout_alignBottom="@+id/btFile"
        android:layout_toRightOf="@+id/btFile"
        android:layout_toEndOf="@+id/btFile" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upload your resume"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="40dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/btFile"
        android:layout_alignStart="@+id/btFile"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="43dp" />

    <Button
        android:id="@+id/btLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btFile"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="85dp"
        android:text="Logout" />

</RelativeLayout>
